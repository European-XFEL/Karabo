DEP_NAME="parse-1.6.3"
DEP_EXT="tar.gz"
EXTRACT_COMMAND="tar -xzf $DEP_NAME.$DEP_EXT"
CONFIGURE_COMMAND=
if [ "$(uname -s)" == "Darwin" ]; then
MAKE_COMMAND=
INSTALL_COMMAND="/opt/local/Library/Frameworks/Python.framework/Versions/2.7/bin/python setup.py install --user"
else
MAKE_COMMAND="$INSTALL_PREFIX/bin/python setup.py build"
INSTALL_COMMAND="$INSTALL_PREFIX/bin/python setup.py install --prefix=$INSTALL_PREFIX"
fi
