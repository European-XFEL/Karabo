DEP_NAME="tiff-4.0.6"
DEP_EXT="tar.gz"
EXTRACT_COMMAND="tar -xzf $DEP_NAME.$DEP_EXT"
CONFIGURE_COMMAND="./configure --prefix=$INSTALL_PREFIX --libdir=$INSTALL_PREFIX/lib --with-jpeg-include-dir=$INSTALL_PREFIX/include --with-jpeg-lib-dir=$INSTALL_PREFIX/lib --enable-shared"
MAKE_COMMAND="make clean && make -j$NUM_CORES"
INSTALL_COMMAND="make install"
