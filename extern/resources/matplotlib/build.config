DEP_NAME="matplotlib-1.2.1"
DEP_EXT="tar.gz"
EXTRACT_COMMAND="tar -xzf $DEP_NAME.$DEP_EXT"
echo "[directories]" > $DEP_NAME/setup.cfg
echo "basedirlist = $INSTALL_PREFIX" >> $DEP_NAME/setup.cfg
echo "[rc_options]" >> $DEP_NAME/setup.cfg
echo "backend = Qt4Agg" >> $DEP_NAME/setup.cfg
basedirlist = $INSTALL_PREFIX
MAKE_COMMAND="$INSTALL_PREFIX/bin/python setup.py build"
INSTALL_COMMAND="$INSTALL_PREFIX/bin/python setup.py install --prefix=$INSTALL_PREFIX"
