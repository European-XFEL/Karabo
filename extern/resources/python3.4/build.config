DEP_NAME="Python-3.4.3"
DEP_EXT="tar"
EXTRACT_COMMAND="tar -xf $DEP_NAME.$DEP_EXT"
CONFIGURE_COMMAND="./configure --prefix=$INSTALL_PREFIX --libdir=$INSTALL_PREFIX/lib --without-ensurepip --enable-shared LDFLAGS=-Wl,-rpath,\\\\\\\$\\\$ORIGIN/../..\ -Wl,-rpath,\\\\\\\$\\\$ORIGIN/../lib"
MAKE_COMMAND="make -j"
INSTALL_COMMAND="make install; cd $INSTALL_PREFIX/bin; ln -fs python3 python"
