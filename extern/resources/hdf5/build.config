DEP_NAME="hdf5-1.8.12"
DEP_EXT="tar.gz"
EXTRACT_COMMAND="tar -xzf $DEP_NAME.$DEP_EXT"
CONFIGURE_COMMAND="./configure --prefix=$INSTALL_PREFIX --libdir=$INSTALL_PREFIX/lib --includedir=$INSTALL_PREFIX/include/hdf5 --enable-production --with-zlib --enable-threadsafe --with-pthread=/usr"
MAKE_COMMAND="make -j$NUM_CORES"
INSTALL_COMMAND="make install"
