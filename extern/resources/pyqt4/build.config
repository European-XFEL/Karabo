DEP_NAME="PyQt-x11-gpl-4.10.3"
DEP_EXT="tar.gz"
EXTRACT_COMMAND="tar -xzf $DEP_NAME.$DEP_EXT"
CONFIGURE_COMMAND="$INSTALL_PREFIX/bin/python3 configure.py --confirm-license --no-designer-plugin --no-qsci-api"
if [ "$DISTRO_ID" == "Scientific" -o "$DISTRO_ID" == "CentOS" ]; then
    CONFIGURE_COMMAND="$INSTALL_PREFIX/bin/python configure.py --assume-shared --confirm-license --no-designer-plugin --no-qsci-api -q /usr/bin/qmake-qt4"
fi
MAKE_COMMAND="make -j$NUM_CORES"
INSTALL_COMMAND="make install"
