DEP_NAME="PyQwt-5.2.0"
DEP_EXT="tar.gz"
EXTRACT_COMMAND="tar -xzf $DEP_NAME.$DEP_EXT; patch -p0 < patch1"
CONFIGURE_COMMAND="cd configure; $INSTALL_PREFIX/bin/2to3 -w configure.py;$INSTALL_PREFIX/bin/python3 configure.py -Q ../qwt-5.2"
MAKE_COMMAND="make -j$NUM_CORES"
INSTALL_COMMAND="make install"
