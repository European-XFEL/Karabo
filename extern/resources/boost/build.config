DEP_NAME="boost_1_82_0"
DEP_EXT="tar"

BOOST_LIBRARIES="date_time,chrono,filesystem,regex,thread,system,program_options,python"
CONFIGURE_COMMAND="ln -s $INSTALL_PREFIX/include/python3.8m $INSTALL_PREFIX/include/python3.8; ./bootstrap.sh --prefix=$INSTALL_PREFIX --libdir=$INSTALL_PREFIX/lib --with-libraries=$BOOST_LIBRARIES --with-python=$INSTALL_PREFIX/bin/python3 --with-python-root=$INSTALL_PREFIX"

EXTRACT_COMMAND="tar -xf $DEP_NAME.$DEP_EXT"

MAKE_COMMAND="./b2 --disable-icu -a variant=release  cxxflags='-std=c++14' -j$NUM_CORES install"

INSTALL_COMMAND=
