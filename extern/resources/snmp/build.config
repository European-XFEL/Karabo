DEP_NAME="net-snmp-5.7"
DEP_EXT="tar.gz"
EXTRACT_COMMAND="tar -xzf $DEP_NAME.$DEP_EXT"
CONFIGURE_COMMAND="./configure --prefix=$INSTALL_PREFIX --libdir=$INSTALL_PREFIX/lib --with-default-snmp-version=2 --with-sys-contact=\"European XFEL GmbH\" --with-sys-location=Unknown --with-logfile=/var/log/snmpd.log --with-persistent-directory=/var/net-snmp --without-perl-modules --disable-embedded-perl"
MAKE_COMMAND="make -j"
INSTALL_COMMAND="make install"
