# The source of traitles requires `flit` to build.
# It will not build successfully so we downloaded
# wheel from https://pypi.org/project/pyparsing/#files
# and processed it with the following steps
# $ export DEP_NAME="traitlets-5.2.0"
# $ mkdir -p $DEP_NAME/downloaded
# $ unzip $DEP_NAME-py3-none-any.whl -d $DEP_NAME/downloaded
# $ tar cf $DEP_NAME.tar $DEP_NAME

DEP_NAME="traitlets-5.2.0"
DEP_EXT="tar"
EXTRACT_COMMAND="tar -xf $DEP_NAME.$DEP_EXT; pushd $DEP_NAME/downloaded; ls; zip -r ../$DEP_NAME-py3-none-any.whl . ;popd"
CONFIGURE_COMMAND=""
MAKE_COMMAND=""
INSTALL_COMMAND="echo $PWD; ls ;$INSTALL_PREFIX/bin/python3 -m pip install $DEP_NAME-py3-none-any.whl --no-deps --prefix=$INSTALL_PREFIX"
