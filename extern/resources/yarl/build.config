DEP_NAME="yarl-1.6.3"
DEP_EXT="tar"
EXTRACT_COMMAND="tar -xf $DEP_NAME.$DEP_EXT"
CONFIGURE_COMMAND=
MAKE_COMMAND="YARL_NO_EXTENSIONS=1 $INSTALL_PREFIX/bin/python3 setup.py build"
INSTALL_COMMAND="YARL_NO_EXTENSIONS=1 $INSTALL_PREFIX/bin/python3 setup.py install --prefix=$INSTALL_PREFIX"
