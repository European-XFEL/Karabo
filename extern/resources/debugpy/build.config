# https://pypi.org/project/debugpy/#description
# from zip file made a tar
# $ unzip debugpy-1.6.0.zip
# $ tar cf debugpy-1.6.0.tar debugpy-1.6.0
DEP_NAME="debugpy-1.6.0"
DEP_EXT="tar"
EXTRACT_COMMAND="tar -xf $DEP_NAME.$DEP_EXT"
CONFIGURE_COMMAND=
MAKE_COMMAND="$INSTALL_PREFIX/bin/python3 setup.py build"
INSTALL_COMMAND="$INSTALL_PREFIX/bin/python3 setup.py install --prefix=$INSTALL_PREFIX"
