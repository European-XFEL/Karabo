DEP_NAME="snappy-1.1.2"
DEP_EXT="tar.gz"
EXTRACT_COMMAND="tar -xzf $DEP_NAME.$DEP_EXT"
CONFIGURE_COMMAND="./configure --prefix=$INSTALL_PREFIX --disable-static --enable-dependency-tracking --disable-gtest CXXFLAGS=\"-O3 -DNDEBUG\" --libdir=$INSTALL_PREFIX/lib"
MAKE_COMMAND="make; make check"
INSTALL_COMMAND="make install"
