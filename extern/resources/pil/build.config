DEP_NAME="Imaging-1.1.7"
DEP_EXT="tar.gz"
 EXTRACT_COMMAND="tar xvzf $DEP_NAME.$DEP_EXT"
CONFIGURE_COMMAND="[ ! -f setup_${DISTRO_ID} ] && patch -p1 -i ../${DISTRO_ID}.patch -o setup_${DISTRO_ID}.py && patch -p1 < ../imagingft.patch"
MAKE_COMMAND="$INSTALL_PREFIX/bin/python setup_${DISTRO_ID}.py build_ext -i"
INSTALL_COMMAND="$INSTALL_PREFIX/bin/python setup_${DISTRO_ID}.py install --prefix=$INSTALL_PREFIX"
