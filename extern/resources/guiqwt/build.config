DEP_NAME="guiqwt-2.2.0"
DEP_EXT="zip"
EXTRACT_COMMAND="unzip -o $DEP_NAME.$DEP_EXT; patch $DEP_NAME/setup.py < setup_py.patch"
CONFIGURE_COMMAND=
MAKE_COMMAND="$INSTALL_PREFIX/bin/python setup.py build --fcompiler=gnu95"
INSTALL_COMMAND="$INSTALL_PREFIX/bin/python setup.py install --prefix=$INSTALL_PREFIX"
