DEP_NAME="guiqwt-3.0.3"
DEP_EXT="tar"
EXTRACT_COMMAND="tar xf $DEP_NAME.$DEP_EXT"
CONFIGURE_COMMAND="patch -p1 < ../curve.patch"
if [ "$(uname -s)" == "Darwin" ]; then
MAKE_COMMAND=
INSTALL_COMMAND="export LC_ALL=en_US.UTF-8; export LANG=en_US.UTF-8; /opt/local/bin/python setup.py build install --user"
else
MAKE_COMMAND="$INSTALL_PREFIX/bin/python3 setup.py build"
INSTALL_COMMAND="$INSTALL_PREFIX/bin/python3 setup.py install --prefix=$INSTALL_PREFIX"
fi
