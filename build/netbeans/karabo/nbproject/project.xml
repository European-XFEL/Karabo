<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://www.netbeans.org/ns/project/1">
    <type>org.netbeans.modules.cnd.makeproject</type>
    <configuration>
        <data xmlns="http://www.netbeans.org/ns/make-project/1">
            <name>karabo</name>
            <c-extensions/>
            <cpp-extensions>cc,cpp</cpp-extensions>
            <header-extensions>h,hh,hpp</header-extensions>
            <sourceEncoding>UTF-8</sourceEncoding>
            <make-dep-projects/>
            <sourceRootList>
                <sourceRootElem>../../../src/karabo</sourceRootElem>
            </sourceRootList>
            <confList>
                <confElem>
                    <name>Debug</name>
                    <type>2</type>
                </confElem>
                <confElem>
                    <name>Release</name>
                    <type>2</type>
                </confElem>
            </confList>
        </data>
        <preferences xmlns="http://www.netbeans.org/ns/auxiliary-configuration-preferences/1">
            <module name="org-netbeans-modules-editor-indent">
                <node name="CodeStyle"/>
            </module>
            <module name="org-netbeans-modules-hudson">
                <property name="builder" value="http://exflpcx18295:8080/job/karabo-framework%20on%20Ubuntu12.04/"/>
            </module>
        </preferences>
        <spellchecker-wordlist xmlns="http://www.netbeans.org/ns/spellchecker-wordlist/1">
            <word>std</word>
        </spellchecker-wordlist>
    </configuration>
</project>
